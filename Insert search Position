class Solution {
    public int searchInsert(int[] n, int t) {
        int ans=0;
        if (n[n.length-1]==t)return n.length-1;
        if(t>n[n.length-1])return n.length;
        for(int i=0;i<n.length-1;i++){
            if(n[i]==t){
                return i;
            }
            if(n[i]<t && n[i+1]>t){
            ans=i+1;
            break;
        }
    }
        return ans;
}
}
